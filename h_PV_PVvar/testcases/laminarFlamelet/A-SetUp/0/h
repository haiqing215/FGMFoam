{
    closed
    {
        //type            zeroGradient;
        type            lookUpEnthalpy;
        h0              $internalField;
        value           $internalField;
        //type            calculated;
        //value           $internalField;
    }
    open
    {
        type            zeroGradient;
        //type            calculated;
        //value           $internalField;
    }
    sides
    {
        type            empty;
    }
}


