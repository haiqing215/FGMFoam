writePrecision    9;
writeCompression  off;
timeFormat        general;
timePrecision     11;
runTimeModifiable true;
adjustTimeStep    no;
maxCo             1.0;
maxDeltaT         1;

libs ("liblookUpBoundaryCondition.so");

